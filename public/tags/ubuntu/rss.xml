            <item>
                <title>Ubuntu 20.04改变</title>
                <pubDate>Wed, 09 Dec 2020 00:00:00 +0000</pubDate>
                <link>http://127.0.0.1:1111/ubutnu-20-04/</link>
                <guid>http://127.0.0.1:1111/ubutnu-20-04/</guid>
                <description>&lt;blockquote&gt;
&lt;h3 id=&quot;network-manage&quot;&gt;network manage&lt;&#x2F;h3&gt;
&lt;&#x2F;blockquote&gt;
&lt;p&gt;Old&lt;&#x2F;p&gt;
&lt;pre style=&quot;background-color:#2b303b;&quot;&gt;
&lt;span style=&quot;color:#c0c5ce;&quot;&gt; cat &#x2F;etc&#x2F;network&#x2F;interfaces
# Generated by SolusVM

 auto lo
  iface lo inet loopback

 auto eth0
  iface eth0 inet static
   address 10.0.3.26
   gateway 10.0.3.1
   netmask 255.255.255.0
   dns-nameservers 1.1.1.1 1.0.0.1
&lt;&#x2F;span&gt;&lt;&#x2F;pre&gt;
&lt;p&gt;new&lt;&#x2F;p&gt;
&lt;pre style=&quot;background-color:#2b303b;&quot;&gt;
&lt;span style=&quot;color:#c0c5ce;&quot;&gt;cat &#x2F;etc&#x2F;netplan&#x2F;00-install.yaml
network:
 version: 2
 renderer: networkd
 ethernets:
   eth0:
        addresses:
             - 10.0.3.26&#x2F;24
        gateway4: 10.0.3.1
        nameservers:
                addresses:
                      - 1.1.1.1
                      - 8.8.8.8
&lt;&#x2F;span&gt;&lt;&#x2F;pre&gt;
&lt;blockquote&gt;
&lt;h3 id=&quot;package-manager&quot;&gt;package manager&lt;&#x2F;h3&gt;
&lt;&#x2F;blockquote&gt;
&lt;pre style=&quot;background-color:#2b303b;&quot;&gt;
&lt;span style=&quot;color:#c0c5ce;&quot;&gt;snap install docker
